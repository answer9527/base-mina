.tab_container{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.tagImage{
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 10rpx
}
.tagName{
  font-size: 22rpx;
}
.count{
  position: absolute;
  top: -10rpx;
  right: -2rpx;
  font-size: 20rpx;
  background: #F8872A;
  color: #FFFFFF;
  border-radius:50% ;
  /* padding: 2rpx 6rpx; */
  width: 30rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
}